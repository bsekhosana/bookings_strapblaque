{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-pcntl": "*",
        "ext-posix": "*",
        "ext-redis": "*",
        "bugsnag/bugsnag-laravel": "^2.27",
        "emotality/laravel-telegram-logger": "^1.0",
        "emotality/panacea-laravel": "^2.1",
        "geoip2/geoip2": "^3.0",
        "guzzlehttp/guzzle": "^7.8",
        "interaction-design-foundation/laravel-geoip": "^3.5",
        "intervention/image": "^3.6",
        "laravel/framework": "^11.0",
        "laravel/horizon": "^5.24",
        "laravel/nova": "^4.22",
        "laravel/prompts": "^0.1.20",
        "laravel/pulse": "^1.2",
        "laravel/sanctum": "^4",
        "laravel/tinker": "^2.9",
        "laravel/ui": "^4.5",
        "laravelista/ekko": "^4.1",
        "propaganistas/laravel-phone": "^5.3",
        "spatie/laravel-honeypot": "^4.5",
        "symfony/http-client": "^7.0",
        "symfony/mailgun-mailer": "^7.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.13",
        "barryvdh/laravel-ide-helper": "^3.0",
        "emotality/laravel-crud": "dev-main",
        "fakerphp/faker": "^1.23",
        "kitloong/laravel-migrations-generator": "^7.0",
        "larastan/larastan": "^2.9",
        "laravel/envoy": "^2.9",
        "laravel/pint": "^1.15",
        "laravel/sail": "^1.29",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-laravel": "^2.3",
        "spatie/laravel-ignition": "^2.5",
        "spatie/laravel-ray": "^1.36"
    },
    "repositories": [
        {
            "type": "path",
            "url": "./repositories/nova"
        },
        {
            "type": "path",
            "url": "./repositories/laravel-crud"
        }
    ],
    "autoload": {
        "files": [
            "app/Helpers/helper.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --force --quiet",
            "@php artisan flush --no-interaction"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "prod": [
            "@php artisan bootstrap:clear",
            "rm -rf vendor bootstrap/cache/*",
            "composer install --no-interaction --quiet --no-dev --prefer-dist --optimize-autoloader",
            "@php artisan bootstrap:cache"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "platform": {
            "php": "8.3"
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
